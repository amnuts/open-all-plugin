{
  "manifest_version": 3,
  "name": "Open all links in tabs",
  "description": "Paste in all the urls you want to open in tabs",
  "homepage_url": "https://github.com/amnuts/open-all-plugin",
  "version": "1.0.0",
  "icons": {
    "16": "icons/icon16.png",
    "24": "icons/icon24.png",
    "32": "icons/icon32.png",
    "64": "icons/icon64.png",
    "128": "icons/icon128.png",
    "256": "icons/icon256.png"
  },
  "action": {
    "default_popup": "input.html",
    "default_title": "Enter your urls"
  },
  "permissions": [
    "tabs",
    "activeTab",
    "<all_urls>"
  ],
  "host_permissions": [
    "*://*/*"
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  }
}